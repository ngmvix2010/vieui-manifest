<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote name="lineage" fetch="https://github.com/LineageOS/" />
  <remote name="vieui" fetch="https://github.com/ngmvix2010/" revision="main" />

  <default revision="refs/heads/lineage-23.2" remote="lineage" sync-j="4" />

  <project path="external/vieui/sdk" name="vieui-sdk" remote="vieui" />
  <project path="frameworks/base" name="vieui-frameworks-base" remote="vieui" />

  <project path="build/make" name="android_build" groups="pdk,sbox" >
    <linkfile src="CleanSpec.mk" dest="CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
  </project>

</manifest>
