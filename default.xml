<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="github"
           fetch="https://github.com/"
           review="review.lineageos.org" />

  <remote  name="lineage"
           fetch="https://github.com/LineageOS/" />

  <remote  name="vieui"
           fetch="https://github.com/ngmvix2010/"
           revision="refs/heads/main" />

  <default revision="refs/heads/lineage-23.2"
           remote="github"
           sync-j="4" />

  <project path="external/vieui/sdk" name="vieui-sdk" remote="vieui" />

  <remove-project name="LineageOS/android_packages_apps_Settings" />
  <project path="packages/apps/Settings" name="vieui-settings" remote="vieui" />

  <remove-project name="LineageOS/android_frameworks_base" />
  <project path="frameworks/base" name="vieui-frameworks-base" remote="vieui" />

</manifest>
